{
  "name": "sf-node-data-app",
  "version": "1.0.0",
  "description": "SFNode Data app",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/Index.ts -e input/example.xlsx -c input/conf_transform.yaml -o output --includeHeaderNames",
    "start-import": "ts-node src/Index.ts -e input/example.xlsx -c input/conf_import.yaml -o output --includeHeaderNames",
    "start-csv": "ts-node src/Index.ts --csvFiles input/data.csv -c input/conf_csv.yaml -o output",
    "start-csv-big": "ts-node src/Index.ts --csvFiles input/data_big.csv -c input/conf_csv_big.yaml -o output",
    "debug": "node --inspect-brk -r ts-node/register src/Index.ts -e input/example.xlsx -c input/conf_import.yaml -o output --includeHeaderNames",
    "debug-csv": "node --inspect-brk -r ts-node/register src/Index.ts --csvFiles input/data.csv -c input/conf_csv.yaml -o output",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/js-yaml": "^4.0.9",
    "@types/jsforce": "^1.11.5",
    "@types/node": "^22.15.16",
    "@types/papaparse": "^5.3.16",
    "@types/xlsx": "^0.0.35",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "js-yaml": "^4.1.0",
    "jsforce": "^3.8.1",
    "papaparse": "^5.5.2",
    "xlsx": "^0.18.5"
  }
}